{
  "manifest_version": 3,
  "name": "Script Control Panel",
  "version": "1.0",
  "description": "A control panel to manage scripts on a webpage.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www2.x-feeder.info/*"],
      "js": ["content_script.js"]
    }
  ],
  "host_permissions": ["https://www2.x-feeder.info/*"]
}
