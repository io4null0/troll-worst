{
  "manifest_version": 3,
  "name": "worstV1.1",
  "version": "1.0",
  "description": "feeder用のtrolltool",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www2.x-feeder.info/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
